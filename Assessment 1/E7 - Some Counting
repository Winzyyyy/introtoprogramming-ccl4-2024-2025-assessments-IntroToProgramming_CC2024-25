#using range function
for counts1 in range(0, 51 , 1):  # counts up 0 to 50 in increments of 1
    print(counts1)
    
for counts2 in range(50, -1 , -1): #count down from 50 to 0 in decrements of 1
    print(counts2)

for counts3 in range(30, 51, 1): #counts up from 30 to 50 in increments of 1 
    print(counts3)

for counts4 in range(50 , 9 , -2): #counts down from 50 to 10 in decrements of 2
    print(counts4)

for counts5 in range(100 , 201 , 5): #counts up from 100 to 200 in increments of 5
    print(counts5)
